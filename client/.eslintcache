\r\n                  <div class=\"content\">\r\n                      <img class=\"right floated mini ui image\" src={userImg}/>\r\n                      <div class=\"header\">\r\n                          <strong>{request.sender_fName} {request.sender_lName}</strong>\r\n                      </div>\r\n                      <div class=\"description\">\r\n                      {request.sender_fName} wants to friend you\r\n                      </div>\r\n                  </div>\r\n                  <div class=\"extra content\">\r\n                      <div class=\"resultsBtn\">\r\n                        <button class=\"btn btnApprove\" >Approve</button>\r\n                        {/* <button class=\"btn btnApprove\" onClick={() => { \r\n                              addFriend({ variables:  { _id: request._id } }); declineFriend({ variables:  { _id: request._id } });}} >Approve</button> */}\r\n                        <button class=\"btn btnDecline\" onClick={() => { \r\n                              declineFriend({ variables:  { _id: request._id } }); }} >Decline</button>\r\n                      </div>\r\n                  </div>\r\n              </div>\r\n          </div>\r\n        ))}\r\n      </div>\r\n  );\r\n};\r\n\r\nexport default Requests;\r\n",{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"117","replacedBy":"121"},{"ruleId":"119","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":63,"column":5,"nodeType":"125","endLine":63,"endColumn":7,"suggestions":"126"},{"ruleId":"127","severity":1,"message":"128","line":4,"column":8,"nodeType":"129","messageId":"130","endLine":4,"endColumn":15},{"ruleId":"127","severity":1,"message":"131","line":6,"column":9,"nodeType":"129","messageId":"130","endLine":6,"endColumn":20},{"ruleId":"127","severity":1,"message":"132","line":2,"column":10,"nodeType":"129","messageId":"130","endLine":2,"endColumn":18},{"ruleId":"127","severity":1,"message":"133","line":2,"column":2